# Suppress deprecation warnings for transitive dependencies
# These warnings come from ESLint 8's dependency chain
# Will be resolved when upgrading to Next.js 15+ (ESLint 9)
legacy-peer-deps=false
audit-level=moderate

# Retry settings for npm registry issues
fetch-retries=5
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000

